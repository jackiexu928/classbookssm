<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jackie.classbook.dao.NationDao">
    <select id="queryNationById" resultType="Nation" parameterType="long">
        select * from nation
        where id = #{id}
    </select>
</mapper>